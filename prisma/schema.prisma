generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Question {
  id         Int      @id @default(autoincrement())
  text       String
  answer     String
  difficulty String
  createdAt  DateTime @default(now())
}

model Flashcard {
  id        Int      @id @default(autoincrement())
  question  String
  answer    String
  createdAt DateTime @default(now())
}

